    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-styled-components": "^7.1.1",
    "node-dev": "^8.0.0",
    "postcss-syntax": "^0.36.2",
    "pre-commit": "^1.2.2",
