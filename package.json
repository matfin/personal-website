{
  "name": "mattfinucane.com",
  "version": "1.0.0",
  "description": "Personal website built using React and TypeScript",
  "main": "src/index.js",
  "scripts": {
    "server": "node-dev ./dist/server/server.js",
    "webpack": "webpack --config webpack.dev.js --watch --progress --colors --open",
    "clear": "rm -rf dist/app dist/server",
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "coverage": "jest --collect-coverage",
    "csslint": "stylelint './src/app/styles/**' './src/**/*.css.ts'",
    "lint": "eslint ./src/**/*.{ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matfin/personal-website"
  },
  "author": "Matt Finucane <me@mattfinucane.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matfin/personal-website/issues"
  },
  "homepage": "https://github.com/matfin/personal-website#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/react": "^10.0.3",
    "@types/node": "^14.0.1",
    "@types/node-fetch": "^2.5.5",
    "@types/react-redux": "^7.1.7",
    "@types/styled-components": "^5.0.1",
    "@types/testing-library__react": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^25.1.0",
    "node-dev": "^4.0.0",
    "pre-commit": "^1.2.2",
    "redux-mock-store": "^1.5.4",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^6.0.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux": "^3.6.0",
    "babel-loader": "^8.0.6",
    "express": "^4.17.1",
    "jest-styled-components": "^7.0.2",
    "node-fetch": "^2.6.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.0.1",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleNameMapper": {
      "^content/(.*)$": "<rootDir>/assets/content/$1",
      "^app/(.*)$": "<rootDir>/src/app/$1",
      "^common/(.*)$": "<rootDir>/src/common/$1",
      "^server/(.*)$": "<rootDir>/src/server/$1",
      "^svg/(.*)$": "<rootDir>/__mocks__/svgMock.js"
    }
  },
  "pre-commit": [
    "coverage",
    "lint",
    "csslint"
  ]
}
