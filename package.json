{
  "name": "mattfinucane.com",
  "version": "1.11.0",
  "description": "Personal website built using React and TypeScript",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build:client": "vite build --outDir dist",
    "build:static": "vite build --ssr src/entry-static.tsx --outDir dist/static",
    "preview": "vite preview",
    "ssg": "tsx --env-file .env prerender",
    "deploy": "yarn build:client && yarn build:static && yarn ssg",
    "clean-deps": "rm -rf node_modules yarn.lock",
    "clear": "rm -rf ./dist ./dev-dist",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:coverage:ci": "vitest run --coverage",
    "checks": "yarn test:coverage:ci && yarn lint && yarn lint:css",
    "lint:css": "stylelint './src/**/*.css.tsx'",
    "lint": "yarn biome lint ./src",
    "lint:fix": "yarn biome lint --write ./src",
    "format": "yarn biome format --write ./src",
    "nuke": "yarn clean-deps && yarn",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matfin/personal-website"
  },
  "author": "Matt Finucane <me@mattfinucane.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matfin/personal-website/issues"
  },
  "homepage": "https://github.com/matfin/personal-website#readme",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.8.4",
    "@types/react": "^19.0.5",
    "@types/react-dom": "^19.0.3",
    "@types/react-redux": "^7.1.33",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.1.4",
    "globals": "^15.12.0",
    "husky": "^9.0.11",
    "jsdom": "^26.0.0",
    "postcss-styled-syntax": "^0.7.0",
    "stylelint": "^16.13.0",
    "stylelint-config-standard": "^37.0.0",
    "tsx": "^4.10.2",
    "typescript": "^5.4.3",
    "typescript-eslint": "^8.13.0",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.1.1",
    "styled-components": "^6.1.8"
  }
}
