{
  "name": "mattfinucane.com",
  "version": "1.8.0",
  "description": "Personal website built using React and TypeScript",
  "scripts": {
    "clean-deps": "rm -rf node_modules yarn.lock",
    "checks": "yarn test:coverage && yarn lint && yarn csslint",
    "ssg": "node ./dist/generate.js",
    "deploy": "yarn build && yarn ssg",
    "start": "yarn clear && webpack serve --config webpack.dev.js",
    "clear": "rm -rf ./dist ./out",
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "ci:coverage": "jest --collect-coverage --runInBand --no-cache --watchAll=false",
    "csslint": "stylelint --config ./stylelint.config.js './src/**/*.css.tsx'",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "nuke": "yarn clean-deps && yarn",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matfin/personal-website"
  },
  "author": "Matt Finucane <me@mattfinucane.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matfin/personal-website/issues"
  },
  "homepage": "https://github.com/matfin/personal-website#readme",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/react": "^14.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.3",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.23",
    "@types/react-helmet": "^6.1.5",
    "@types/react-redux": "^7.1.33",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-styled-components": "^7.1.1",
    "node-dev": "^8.0.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.2.5",
    "stylelint": "^16.3.1",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^5.0.4",
