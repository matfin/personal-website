    "@reduxjs/toolkit": "^2.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.22.3",
    "styled-components": "^6.1.8",
    "uuid": "^9.0.0"
  }
}
