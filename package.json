{
  "name": "mattfinucane.com",
  "version": "1.6.6",
  "description": "Personal website built using React and TypeScript",
  "scripts": {
    "clean-deps": "rm -rf node_modules yarn.lock",
    "checks": "yarn coverage && yarn lint && yarn csslint",
    "ssg": "node ./dist/generate.js",
    "deploy": "yarn build && yarn ssg",
    "dev": "yarn clear && webpack serve --config webpack.dev.js",
    "clear": "rm -rf ./dist ./out",
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "coverage": "jest --collect-coverage",
    "ci:coverage": "jest --collect-coverage --runInBand --no-cache --watchAll=false",
    "csslint": "stylelint --config ./stylelint.config.js './src/**/*.css.tsx'",
    "lint": "eslint \"./src/**/*.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matfin/personal-website"
  },
  "author": "Matt Finucane <me@mattfinucane.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matfin/personal-website/issues"
  },
  "homepage": "https://github.com/matfin/personal-website#readme",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@redux-saga/testing-utils": "^1.1.5",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-helmet": "^6.1.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.1.8",
    "@types/redux": "^3.6.0",
    "@types/redux-mock-store": "^1.0.3",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.36.0",
