{
  "name": "mattfinucane.com",
  "version": "1.6.0",
  "description": "Personal website built using React and TypeScript",
  "scripts": {
    "clean-deps": "rm -rf node_modules yarn.lock",
    "checks": "yarn coverage && yarn lint && yarn csslint",
    "ssg": "node ./dist/generate.js",
    "deploy": "yarn build && yarn ssg",
    "dev": "yarn clear && webpack serve --config webpack.dev.js",
    "clear": "rm -rf ./dist ./out",
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "coverage": "jest --collect-coverage",
    "csslint": "stylelint \"./src/app/styles/**/*\" \"./src/**/*.css.tsx\"",
    "lint": "eslint \"./src/**/*.{ts,tsx}\""
  },
