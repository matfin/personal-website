{
  "name": "mattfinucane.com",
  "version": "1.9.3",
  "description": "Personal website built using React and TypeScript",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build:client": "vite build --outDir dist",
    "build:static": "vite build --ssr src/entry-static.tsx --outDir dist/static",
    "preview": "vite preview",
    "ssg": "tsx --env-file .env prerender",
    "deploy": "yarn build:client && yarn build:static && yarn ssg",
    "clean-deps": "rm -rf node_modules yarn.lock",
    "clear": "rm -rf ./dist ./dev-dist",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:coverage:ci": "vitest run --coverage",
    "checks": "yarn test:coverage:ci && yarn lint && yarn lint:css",
    "lint:css": "stylelint './src/**/*.css.tsx'",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "nuke": "yarn clean-deps && yarn",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matfin/personal-website"
  },
  "author": "Matt Finucane <me@mattfinucane.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matfin/personal-website/issues"
  },
  "homepage": "https://github.com/matfin/personal-website#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^14.2.2",
    "@types/node": "^20.12.3",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.5",
    "@types/react-redux": "^7.1.33",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-barrel-files": "^2.0.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "postcss-styled-syntax": "^0.6.4",
    "prettier": "^3.2.5",
    "stylelint": "^16.6.0",
    "stylelint-config-standard": "^36.0.0",
    "tsx": "^4.10.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "styled-components": "^6.1.8"
  }
}
